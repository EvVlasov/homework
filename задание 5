public class homework {
    private static java.util.Scanner input;

    public static void main(String[] args) throws java.io.IOException {

        input = new java.util.Scanner(System.in);
int number=0;
        int dividers = 0;
        while(number!=1) {
            System.out.println("if u want to finish this program enter 1");
            System.out.println("Enter ur munber:");
            number = input.nextInt();
            if (number == 1) break;
            for (int i = 1; i <= number; i++) {
                if (number % 2 == 0) {
                    dividers++;
                }
            }
            if (dividers <= 2) System.out.println("simple");
            else System.out.println("compound");

        }
        }
        }
